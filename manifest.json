{
  "manifest_version": 3,
  "name": "Sophia, the AI Shopping Assistant",
  "description": "Sophia gives buying advise for any product categoy on Amazon",
  "version": "1.0",
  "action": {
    "default_popup": "index.html"
  },
  "icons": {
    "16": "icon/icon-16.png",
    "48": "icon/icon-48.png",
    "128": "icon/icon-128.png"
  },
  "permissions": ["scripting", "activeTab", "storage"]

}